<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 타입 별칭 설정============================  -->
	<typeAliases>
		<typeAlias type="com.memo.model.MemoVO" alias="Memo"/>
		<typeAlias type="com.multi.model.CategoryVO" alias="Category"/>
		<typeAlias type="com.multi.model.ProductVO" alias="Product"/>
		<typeAlias type="com.multi.model.CartVO" alias="Cart"/>
		<typeAlias type="com.multi.model.MemberVO" alias="Member"/>
		<typeAlias type="com.multi.model.BoardVO" alias="Board"/>
		<typeAlias type="com.multi.model.PagingVO" alias="Paging"/>
	</typeAliases>
	<!-- 데이터베이스 트랜잭션 관리자 설정=========== -->
	
	<!-- 매퍼 정의================================ -->
	<mappers>
		<!-- <mapper resource="config/mapper/SampleMapper.xml"/> -->
		
		<mapper resource="config/mapper/MemoMapper.xml"/>
		<mapper resource="config/mapper/ProductMapper.xml"/>
		<mapper resource="config/mapper/CartMapper.xml"/>
		<mapper resource="config/mapper/MemberMapper.xml"/>
		<mapper resource="config/mapper/BoardMapper.xml"/>
	</mappers>
</configuration>